.row.wrapper
  .form-wrap.col-md-4.col-md-offset-2
    %h2 Log In To Yelp
    .subhead-wrap
      .subheading New to Yelp? 
      %a Sign Up
    %form
      .form-group
        %input.form-control{placeholder: "Email address", type: "email"}
      .form-group
        %input.form-control{placeholder: "Password", type: "password"}
      .form-group
        %input.btn.btn-block.btn-danger{type: "submit", value: "Log In"}
  .col-md-4
    %img(src="/images/signup_illustration.png")